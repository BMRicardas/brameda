{
  "name": "brameda-astro",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "sync": "astro sync",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "compile": "tsc",
    "lint": "npm run lint:code && npm run lint:format && npm run compile -- --noEmit",
    "lint:fix": "npm run lint:code:fix && npm run lint:format:fix",
    "lint:code": "npx eslint src/** --report-unused-disable-directives",
    "lint:code:fix": "npm run lint:code -- --fix",
    "lint:format": "npx prettier --check \"src/**\"",
    "lint:format:fix": "npx prettier --write \"src/**\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/react": "^4.4.1",
    "@astrojs/sitemap": "^3.6.0",
    "@contentful/rich-text-html-renderer": "^17.1.5",
    "@contentful/rich-text-plain-text-renderer": "^17.1.5",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^5.2.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "astro": "^5.15.3",
    "clsx": "^2.1.1",
    "contentful": "^11.8.9",
    "embla-carousel-react": "^8.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.0",
    "eslint-plugin-astro": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "prettier-plugin-astro": "^0.14.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
